{
  "$schema": "https://json-schema.org/schema",
  "$id": "Environment",
  "title": "Create a Supabase environment",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "The Supabase project to add the environment to",
      "x-prompt": "Which Supabase project?"
    },
    "name": {
      "type": "string",
      "description": "Environment name (e.g., local, production, staging)",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What is the environment name?"
    }
  },
  "required": ["project", "name"]
}
