{
  "name": "@gridatek/nx-supabase",
  "version": "0.5.0",
  "description": "Nx plugin for Supabase integration",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gridatek/nx-supabase.git"
  },
  "keywords": [
    "nx",
    "nx-plugin",
    "supabase",
    "monorepo"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@gridatek/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/nx-supabase/dist",
          "main": "packages/nx-supabase/src/index.ts",
          "tsConfig": "packages/nx-supabase/tsconfig.lib.json",
          "rootDir": "packages/nx-supabase/src",
          "generatePackageJson": false,
          "assets": [
            {
              "input": "./packages/nx-supabase/src",
              "glob": "**/!(*.ts)",
              "output": "."
            },
            {
              "input": "./packages/nx-supabase/src",
              "glob": "**/*.d.ts",
              "output": "."
            },
            {
              "input": ".",
              "glob": "LICENSE",
              "output": ".."
            },
            {
              "input": ".",
              "glob": "README.md",
              "output": ".."
            }
          ]
        }
      }
    }
  },
  "dependencies": {
    "@nx/devkit": "22.1.3",
    "tslib": "^2.3.0"
  },
  "generators": "./generators.json",
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "generators.json",
    "executors.json",
    "LICENSE",
    "README.md"
  ],
  "executors": "./executors.json"
}
